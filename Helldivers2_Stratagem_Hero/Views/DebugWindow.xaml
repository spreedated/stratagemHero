<Window x:Class="Helldivers2_Stratagem_Hero.Views.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Helldivers2_Stratagem_Hero.Views"
        mc:Ignorable="d"
        Title="Debug"
        Height="450"
        Width="197"
        WindowStartupLocation="Manual">
    <Grid>
        <GroupBox Header="Views">
            <StackPanel>
                <Button Content="Start" Click="ButtonStart_Click" />
                <Button Content="RoundEnd" Click="RoundEnd_Click" />
                <Button Content="GameOver" Click="ButtonGameOver_Click" />
                <Button Content="Playername" Click="ButtonPlayername_Click" />
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
